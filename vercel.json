{
  "version": 2,
  "functions": {
    "platform_demo.py": {
      "runtime": "python3.9",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/platform_demo.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
