{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/platform_homepage.html"
    },
    {
      "src": "^/index.html$",
      "dest": "/platform_homepage.html"
    },
    {
      "src": "^/trial/?$",
      "dest": "/trial.html"
    },
    {
      "src": "^/pricing/?$",
      "dest": "/pricing.html"
    },
    {
      "src": "^/contact/?$",
      "dest": "/contact.html"
    },
    {
      "src": "^/login/?$",
      "dest": "/login.html"
    },
    {
      "src": "^/dashboard/?$",
      "dest": "/dashboard.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1"
    }
  ],
  "env": {
    "STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key",
    "STRIPE_MONTHLY_CHECKOUT_URL": "@stripe_monthly_checkout_url",
    "STRIPE_YEARLY_CHECKOUT_URL": "@stripe_yearly_checkout_url"
  }
}
