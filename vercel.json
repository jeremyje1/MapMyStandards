{
  "version": 2,
  "name": "mapmystandards-web",
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "." } }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/about", "destination": "/web/about/index.html" },
    { "source": "/services", "destination": "/web/services/index.html" },
    { "source": "/privacy-policy", "destination": "/web/privacy-policy/index.html" },
    { "source": "/user-guide", "destination": "/web/user-guide/index.html" },
    { "source": "/homepage", "destination": "/web/homepage.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/login", "destination": "https://api.mapmystandards.ai/login", "permanent": false },
    { "source": "/dashboard", "destination": "https://api.mapmystandards.ai/dashboard", "permanent": false }
  ],
  "cleanUrls": true
}
