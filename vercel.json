{
  "version": 2,
  "builds": [
    {
      "src": "platform_demo.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/platform_demo.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
