{
  "outputDirectory": "web",
  "buildCommand": "",
  "devCommand": "",
  "installCommand": "",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.mapmystandards.ai/api/:path*"
    },
    {
      "source": "/auth-simple/:path*",
      "destination": "https://api.mapmystandards.ai/auth-simple/:path*"
    },
    {
      "source": "/signup",
      "destination": "/trial-signup-flow.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard-fixed.html"
    },
    {
      "source": "/",
      "destination": "/homepage-enhanced.html"
    }
  ]
}